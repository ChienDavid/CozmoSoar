sp {test-agent*propose*point-nav-paperclip
   (state <s> ^name test-agent-point-nav
              ^io.input-link <in>)
   (<in> ^object <obj>)
   (<obj> ^cube_id 1
          ^object_id <id>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name go-to-object
         ^actions.go-to-object <a>)
   (<a> ^target_object_id <id>)
}

