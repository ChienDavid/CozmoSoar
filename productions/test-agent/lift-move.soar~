sp {test-agent*propose*lift-move-up
   (state <s> ^name test-agent-lift-move
              ^io <io>)
   (<io>  ^input-link <in>)
   (<in>  ^lift.ratio < 0.9)
   (<in>  ^face_count > 0)
-->
   (<s> ^operator <op> + =)
   (<op> ^name lift-move-up
         ^actions.move-lift <ml>)
   (<ml> ^height 1.0)
   (write (crlf) | PROPOSING LIFT MOVE UP | )
}

sp {test-agent*propose*lift-move-down
   (state <s> ^name test-agent-lift-move
              ^io <io>)
   (<io>  ^input-link <in>)
   (<in>  ^lift.ratio > 0.1)
   (<in>  ^face_count 0)
-->
   (<s> ^operator <op> + =)
   (<op> ^name lift-move-down
         ^actions.move-lift <ml>)
   (<ml> ^height 0.00)
   (write (crlf) | PROPOSING LIFT MOVE DOWN | )
}

sp {test-agent*propose*set-backpack-lights-green
    (state <s> ^name test-agent
               ^io.input-link <in>)
    (<in>  ^face_count > 0)
-->
    (<s> ^operator <op> + =)
    (<op> ^name set-backpack-lights-green
          ^actions.set-backpack-lights <a>)
    (<a> ^color green)
}

sp {test-agent*propose*set-backpack-lights-red
    (state <s> ^name test-agent
               ^io.input-link <in>)
    (<in>  ^face_count 0)
-->
    (<s> ^operator <op> + =)
    (<op> ^name set-backpack-lights-green
          ^actions.set-backpack-lights <a>)
    (<a> ^color red)
}

sp {test-agent*propose*turn-in-place
    (state <s> ^name test-agent
               ^io.input-link <in>)
    (<in> ^object <obj>)
    (<obj> ^visible 1
           ^cube_id 1)
-->
    (<s> ^operator <op> + =)
    (<op> ^name turn-in-place
          ^actions true
          ^angle 90
          ^speed 30)
}

sp {test-agent*apply*turn-in-place
    (state <s> ^operator <op>
              ^io.output-link <out>
              -^performing-action)
    (<op> ^name turn-in-place
          ^actions true
          ^angle <ang>
          ^speed <spd>)
-->
    (<out> ^turn-in-place <a>)
    (<a> ^angle <ang>
         ^speed <spd>)
    (<s> ^performing-action true)
}